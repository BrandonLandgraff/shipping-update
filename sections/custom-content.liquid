{{ 'section-custom-content.css' | asset_url | stylesheet_tag }}

{% liquid
  assign enable_swipe_mobile = section.settings.enable_swipe_mobile
  assign color_scheme = section.settings.color_scheme
%}

<div
  id="m-custom-{{ section.id }}"
  class="m-section m-custom-content {{ section.settings.custom_class }} m-section--padding m-color-{{ color_scheme }} m-gradient{% if section.settings.boxed_layout %} m-section--boxed{% endif %}{% if section.settings.header_divider %} m-section--header-separated{% endif %}"
  data-section-type="custom-content"
  data-section-id="{{ section.id }}"
  style="--column-gap: {{ section.settings.gap }}px; --column-gap-mobile: {{ section.settings.gap_mobile }}px; --row-gap: {{ section.settings.row_gap }}px; --row-gap-mobile: {{ section.settings.row_gap_mobile }}px;--section-padding-top: {{ section.settings.padding_top }}px; --section-padding-bottom: {{ section.settings.padding_bottom }}px;"
>
  <div class="m-page-width">
    <div class="m-section__wrapper{% if section.settings.boxed_layout %} m-gradient m-color-{{ section.settings.boxed_color_scheme }}{% endif %}">
      {%- assign block_width_mobile = '' -%}
      {% render 'index-section-header', section_settings: section.settings %}

      {% if section.blocks.size > 0 %}
        <div class="m-section__content">
          <m-custom-content class="m-custom-content__wrapper m:flex m:flex-wrap">
            {% for block in section.blocks %}
              {% liquid
                assign vertical_alignment = block.settings.alignment
                case block.settings.width
                  when '16%'
                    assign block_width = 'lg:m:w-2/12'
                    assign block_width_mobile = 'md:w-1/2 m:w-full'
                  when '25%'
                    assign block_width = 'lg:m:w-1/4'
                    assign block_width_mobile = 'md:m:w-1/2 m:w-full'
                  when '33%'
                    assign block_width = 'lg:m:w-1/3'
                    assign block_width_mobile = 'm:w-full'
                  when '50%'
                    assign block_width = 'lg:m:w-1/2'
                    assign block_width_mobile = 'm:w-full'
                  when '66%'
                    assign block_width = 'lg:m:w-8/12'
                    assign block_width_mobile = 'm:w-full'
                  when '75%'
                    assign block_width = 'lg:m:w-9/12'
                    assign block_width_mobile = 'm:w-full'
                  when '83%'
                    assign block_width = 'lg:m:w-10/12'
                    assign block_width_mobile = 'm:w-full'
                  when '100%'
                    assign block_width = 'lg:m:w-full'
                    assign block_width_mobile = 'm:w-full'
                endcase

                assign animation_effect = settings.animations
                assign animated = false
                if animation_effect != 'none'
                  assign animated = true
                  if block.settings.animations != 'inherit'
                    assign animation_effect = block.settings.animations
                  endif
                endif

                assign heading_size = block.settings.text_size
                assign desc_color = 'm:text-color-' | append: block.settings.text_color

                assign enable_swipe_mobile = block.settings.enable_swipe_mobile
                assign mobile_layout = block.settings.mobile_layout
              %}

              <div
                id="m-custom__block--{{ block.id }}"
                class="m-custom-content__block m-custom-content__block-{{ block.type }} m:column {{ block_width }} {{ block_width_mobile }} m:display-flex m:items-{{ vertical_alignment }}{{ block.settings.block_custom_class | prepend: ' ' }}"
                {{ block.shopify_attributes }}
              >
                <div class="m-custom-content__block-inner m:w-full{% if animated and animation_effect == 'zoom-fade' %} m:overflow-hidden{% endif %}">
                  {% case block.type %}
                    {% when 'image' %}
                      <div
                        class="m:w-full m:block{% if animated %} m-scroll-trigger animate--{{ animation_effect }}{% endif %}"
                        {% if animated %}
                          data-cascade
                          style="--animation-order: {{ forloop.index }};"
                        {% endif %}
                      >
                        {% if block.settings.image != blank %}
                          <{% if block.settings.link %}a{% else %}div{% endif %}{% if block.settings.link %} href="{{ block.settings.link }}"{% endif %}>
                              {% render 'responsive-image', image: block.settings.image %}
                          </{% if block.settings.link %}a{% else %}div{% endif %}>
                        {% else %}
                          <div class="m-image m-image--placeholder m-image--rounded">
                            {% if block.settings.width == '100%' %}
                              {{
                                'lifestyle-1'
                                | placeholder_svg_tag: 'placeholder-svg m-placeholder-svg m-placeholder-svg--no-border'
                              }}
                            {% else %}
                              {{
                                'image'
                                | placeholder_svg_tag: 'placeholder-svg m-placeholder-svg m-placeholder-svg--no-border'
                              }}
                            {% endif %}
                          </div>
                        {% endif %}
                      </div>
                    {% when 'text' %}
                      <div
                        class="m-richtext m:text-{{ block.settings.align_text }}{% if animated %} m-scroll-trigger animate--{{ animation_effect }}{% endif %}"
                        style="{% if animated %}--animation-order: {{ forloop.index }};{% endif %}"
                        {% if animated %}
                          data-cascade
                        {% endif %}
                      >
                        {% if block.settings.subheading != blank %}
                          <p class="m-richtext__subtitle">{{ block.settings.subheading }}</p>
                        {% endif %}
                        {% if block.settings.title != blank %}
                          <h3 class="m-richtext__title {{ heading_size }}">{{ block.settings.title }}</h3>
                        {% endif %}
                        {% if block.settings.text != blank %}
                          <div class="m-richtext__description rte {{ desc_color }}">{{ block.settings.text }}</div>
                        {% endif %}
                        {% capture arrow  %}<svg width="12" height="12" viewBox="0 0 12 12" fill="none" xmlns="http://www.w3.org/2000/svg"><path d="M4.5 2.25L8.25 6L4.5 9.75" stroke="CurrentColor" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"></path></svg>{% endcapture %}
                        {% capture triangle %}<svg width="12" height="12" viewBox="0 0 12 12" fill="none" xmlns="http://www.w3.org/2000/svg"><path d="M8.51531 5.73474L4.76531 1.98474C4.71287 1.93223 4.64602 1.89647 4.57324 1.88198C4.50046 1.86748 4.42501 1.87491 4.35645 1.90332C4.28789 1.93172 4.22931 1.97984 4.1881 2.04156C4.1469 2.10328 4.12494 2.17584 4.125 2.25005V9.75005C4.12494 9.82426 4.1469 9.89682 4.1881 9.95855C4.22931 10.0203 4.28789 10.0684 4.35645 10.0968C4.42501 10.1252 4.50046 10.1326 4.57324 10.1181C4.64602 10.1036 4.71287 10.0679 4.76531 10.0154L8.51531 6.26536C8.55018 6.23054 8.57784 6.18918 8.59671 6.14365C8.61558 6.09813 8.62529 6.04933 8.62529 6.00005C8.62529 5.95077 8.61558 5.90197 8.59671 5.85645C8.57784 5.81092 8.55018 5.76957 8.51531 5.73474Z" fill="currentColor"/></svg>{% endcapture %}
                        {% liquid 
                          assign is_button_icon = false
                          assign button_label = block.settings.button_label
                          if button_label contains '[arrow]' or button_label contains '[triangle]'
                            assign is_button_icon = true
                          endif
                        %}
                        {% if button_label != blank %}
                          <div class="m-richtext__button content__button">
                            <a
                              href="{{ block.settings.button_link }}"
                              class="m-button {{ block.settings.button_style }}{% if is_button_icon %} m-button--with-icon{% endif %}"
                            >
                              {{- button_label | replace: '[arrow]', arrow | replace: '[triangle]', triangle -}}
                            </a>
                          </div>
                        {% endif %}
                      </div>
                    {% when 'video' %}
                      {{ 'section-video.css' | asset_url | stylesheet_tag }}
                      {% liquid
                        assign autoplay = false
                        if block.settings.autoplay and block.settings.muted
                        endif

                        assign cover_image = false
                        if block.settings.image != blank
                        endif

                        assign text_size = block.settings.text_size
                      %}
                      <div
                        class="m-video{% if animated %} m-scroll-trigger animate--{{ animation_effect }}{% endif %}"
                        data-video-block
                        {% if animated %}
                          data-cascade
                          style="--animation-order: {{ forloop.index }};"
                        {% endif %}
                      >
                        <div class="m-video__wrapper m-video__wrapper--{{ block.id }}">
                          <div
                            class="m-video__inner"
                            style="--aspect-ratio: {{ block.settings.ratio | replace: ':', '/' }}"
                          >
                            <video-component
                              class="deferred-media no-js-hidden"
                              data-media-id="{{ block.settings.video_link.id }}"
                            >
                              <button
                                id="Deferred-Poster-Modal-{{ block.settings.video_link.id }}"
                                class="deferred-media__poster"
                                type="button"
                                aria-label="{{ 'sections.video.play' | t }}"
                              >
                                <span class="deferred-media__poster-button motion-reduce m-video__play m-video__play--style-{{ block.settings.play_style }} m-video__play--size-{{ block.settings.play_size }} m:text-{{ block.settings.text_color }}">
                                  <svg viewBox="0 0 18 18" fill="currentColor">
                                    <path d="M15.562 8.1L3.87.225c-.818-.562-1.87 0-1.87.9v15.75c0 .9 1.052 1.462 1.87.9L15.563 9.9c.584-.45.584-1.35 0-1.8z" fill="currentColor"></path>
                                  </svg>
                                </span>
                                {%- unless block.settings.video_title == blank -%}
                                  <h2 class="m-video__title {{ text_size }}">
                                    {{ block.settings.video_title }}
                                  </h2>
                                {%- endunless -%}
                                {%- if block.settings.image != blank -%}
                                  <div
                                    class="m-video__cover"
                                    style="background-image: url({{ block.settings.image | image_url: width: 1920 }})"
                                  ></div>
                                {%- else -%}
                                  <div class="m-video__cover m-video__cover-placeholder">
                                    {{ 'lifestyle-2' | placeholder_svg_tag: 'm-placeholder-svg m:w-full m:h-full' }}
                                  </div>
                                {%- endif -%}
                              </button>
                              <template>
                                {%- if block.settings.video_type == 'external' -%}
                                  {%- if block.settings.video_link.type == 'youtube' -%}
                                    {% liquid
                                      assign youtubeURL = 'https://www.youtube.com/embed/' | append: block.settings.video_link.id | append: '?enablejsapi=1'
                                      if block.settings.loop
                                        assign youtubeURL = youtubeURL | append: '&loop=1&playlist=' | append: block.settings.video_link.id
                                      endif
                                    %}
                                    <iframe
                                      src="{{ youtubeURL }}"
                                      class="js-youtube"
                                      allow="autoplay; encrypted-media"
                                      allowfullscreen
                                      title="{{ block.settings.video_title | escape }}"
                                    ></iframe>
                                  {%- else -%}
                                    {% liquid
                                      assign vimeoURL = 'https://player.vimeo.com/video/' | append: block.settings.video_link.id
                                      assign hasParam = false
                                      assign vimeoParam = ''

                                      if block.settings.loop
                                        assign hasParam = true
                                        assign vimeoParam = vimeoParam | append: '&loop=1'
                                      endif

                                      if hasParam
                                        assign vimeoURL = vimeoURL | append: '?' | append: vimeoParam
                                      endif
                                    %}
                                    <iframe
                                      src="{{ vimeoURL }}"
                                      class="js-vimeo"
                                      allow="autoplay; encrypted-media"
                                      allowfullscreen
                                      title="{{ block.settings.video_title | escape }}"
                                    ></iframe>
                                  {%- endif -%}
                                {%- else -%}
                                  {% liquid
                                    assign video_url = ''
                                    for source in block.settings.shopify_video.sources
                                      assign format = source.url | slice: -4, 4
                                      if format == '.mp4'
                                        assign video_url = source.url
                                      endif
                                    endfor
                                    assign placeholder_video = 'https://cdn.shopify.com/videos/c/o/v/fbb140fd46e3429eba459e1549dbbe38.mp4'
                                    if video_url == blank
                                      assign video_url = placeholder_video
                                    endif
                                  %}
                                  <video
                                    src="{{ video_url }}"
                                    playsinline
                                    controls
                                    {% if block.settings.loop %}
                                      loop
                                    {% endif %}
                                  ></video>
                                {%- endif -%}
                              </template>
                            </video-component>
                          </div>
                        </div>
                      </div>
                    {% when 'product' %}
                      {{ 'component-product-card.css' | asset_url | stylesheet_tag }}
                      {%- assign product = all_products[block.settings.product] -%}
                      {% if product.title.size > 0 %}
                        {% liquid
                          case settings.pcard_layout
                            when '1'
                              render 'product-card-1', product: product, index: forloop.index, animated: animated, animation_effect: animation_effect
                            when '2'
                              render 'product-card-2', product: product, index: forloop.index, animated: animated, animation_effect: animation_effect
                            when '3'
                              render 'product-card-3', product: product, index: forloop.index, animated: animated, animation_effect: animation_effect
                          endcase
                        %}
                      {% else %}
                        {% comment %}
                          No product yet. Show onboarding one.
                        {% endcomment %}
                        {% render 'product-card-placeholder', index: forloop.index, animated: animated, animation_effect: animation_effect %}
                      {% endif %}
                    {% when 'product_form' %}
                      {% liquid
                        assign product = all_products[block.settings.product]
                        assign current_variant = product.selected_or_first_available_variant
                        assign section_onboarding = false
                        if product == empty
                          assign section_onboarding = true
                        endif
                      %}

                      <div class="m-main-product--info" data-section_onboarding="{{ section_onboarding }}">
                        <div
                          class="m-product-info--wrapper{% if animated %} m-scroll-trigger animate--{{ animation_effect }}{% endif %}"
                          {% if animated %}
                            data-cascade
                            style="--animation-order: {{ forloop.index }};"
                          {% endif %}
                        >
                          {% if block.settings.show_vendor %}
                            <div class="main-product__block main-product__block-vendor">
                              <span>{{ product.vendor }}</span>
                            </div>
                          {% endif %}
                          <div class="main-product__block main-product__block-title">
                            <a
                              class="m-product-title m:text-2xl md:m:text-3xl m:font-medium"
                              href="{{ product.url }}"
                            >
                              {% unless section_onboarding %}
                                {{ product.title }}
                              {% else %}
                                {{ 'homepage.onboarding.product_title' | t }}
                              {% endunless %}
                            </a>
                          </div>
                          {% if block.settings.show_price %}
                            <div class="main-product__block main-product__block-price">
                              {% unless section_onboarding %}
                                {% render 'product-prices', product: product, price_class: 'm:text-xl' %}
                              {% else %}
                                <span class="m:text-xl">{{ 1999 | money }}</span>
                              {% endunless %}
                            </div>
                          {% endif %}
                          {% if block.settings.show_product_review %}
                            <div class="main-product__block main-product__block-rating">
                              {% render 'product-reviews-app__badge', product: product %}
                            </div>
                          {% endif %}
                          {% unless section_onboarding %}
                            <div class="main-product__block main-product__block-description rte">
                              {{ product.description }}
                            </div>
                          {% else %}
                            <div class="main-product__block main-product__block-description rte">
                              {{ 'homepage.onboarding.product_description' | t }}
                            </div>
                          {% endunless %}
                          {% unless section_onboarding %}
                            {%- liquid
                              assign product_form_id = 'product-form-' | append: section.id
                              assign product_form_class = 'product-form-' | append: section.id
                              assign color_scheme = color_scheme
                              if section.settings.boxed_layout
                                assign color_scheme = section.settings.boxed_color_scheme
                              endif
                            -%}
                            {% render 'product-form',
                              section_id: section.id,
                              product: product,
                              section: block,
                              product_form_id: product_form_id,
                              product_form_class: product_form_class,
                              current_variant: current_variant,
                              enable_dynamic_checkout: block.settings.enable_payment_button,
                              show_quantity_selector: block.settings.show_quantity_selector,
                              enableCardButton: block.settings.show_add_to_cart,
                              color_scheme: color_scheme,
                              form_message: true
                            %}
                          {% else %}
                            <div class="main-product__block main-product__block-buy_buttons">
                              <button type="submit" class="m-add-to-cart m-button m-button--primary m:w-full">
                                <span class="m-button__text">{{ 'products.product.add_to_cart' | t }}</span>
                              </button>
                            </div>
                          {% endunless %}
                        </div>
                        {% if product.options_with_values.size > 0 %}
                          <script src="{{ 'variant-picker.js' | asset_url }}" defer="defer"></script>
                        {% endif %}
                      </div>

                    {% when 'image_card' %}
                      {{ 'component-image-card.css' | asset_url | stylesheet_tag }}
                      {% render 'image-card', block: block, classes: block.settings.block_custom_class, animated: animated, animation_effect: animation_effect %}
                    {% when 'product_bundles' %}
                      <div
                        class="product-bundles{% if animated %} m-scroll-trigger animate--{{ animation_effect }}{% endif %}"
                        {% if animated %}
                          data-cascade
                          style="--animation-order: {{ forloop.index }};"
                        {% endif %}
                      >
                        <div class="m-richtext">
                          {% if block.settings.subheading != blank %}
                            <span class="m-richtext__subtitle m:block">{{ block.settings.subheading }}</span>
                          {% endif %}
                          {% if block.settings.title != blank %}
                            <h3 class="m-richtext__title {{ heading_size }}">{{ block.settings.title }}</h3>
                          {% endif %}
                        </div>
                        <div class="product-bundles__list">
                          {%- liquid 
                            assign total_price = 0
                            assign products = block.settings.product_list | reverse | reverse
                          -%}
                          {% if products.size > 0 %}
                          {% for product in products %}
                            {% liquid
                              assign saved_price = ''
                              assign variant = product.first_available_variant
                              if variant.compare_at_price != blank and variant.compare_at_price > variant.price
                                assign saved_price = variant.compare_at_price | minus: variant.price | money
                              endif
                              assign sold_out = false
                              unless variant.available
                                assign sold_out = true
                              endunless
                            %}
                            {% if product != empty %}
                              {% assign total_price = total_price | plus: variant.price %}
                              <div
                                class="product-bundles__item m:display-flex m:items-center{% if block.settings.show_reviews %} product-bundles__item--has-reviews{% endif %}"
                                data-index="{{ forloop.index0 }}"
                              >
                                <span class="product-bundles__item-index m:inline-flex m:justify-center m:items-center">
                                  {{- forloop.index -}}
                                </span>
                                <a href="{{ product.url }}" class="m:block product-bundles__item-image">
                                  {% render 'responsive-image', image: product.featured_image %}
                                </a>
                                <div class="product-bundles__item-info m:display-flex m:flex-wrap md:m:flex-nowrap">
                                  <div class="product-bundles__item-details m:w-full">
                                    {% if block.settings.show_reviews %}
                                      <div class="product-bundles__item-review m:text-sm">
                                        {% render 'product-reviews-app__badge', product: product %}
                                      </div>
                                    {% endif %}
                                    <h3 class="h4">
                                      <a href="{{ product.url }}">{{ product.title }}</a>
                                    </h3>
                                    <div class="product-bundles__item-variant{% if product.has_only_default_variant %} m:hidden{% endif %}">
                                      <select name="id" class="m-product-option--dropdown-select" aria-label="{{ product.title }}">
                                        {% for variant in product.variants %}
                                          <option
                                            value="{{ variant.id }}"
                                            {% if variant.available != true %}
                                              disabled
                                            {% endif %}
                                          >
                                            {{ variant.title }}
                                          </option>
                                        {% endfor %}
                                      </select>
                                    </div>
                                  </div>
                                  <div class="product-bundles__item-prices md:m:text-right">
                                    <p>
                                      {% if saved_price != blank %}
                                        <span class="m:text-color-subtext" data-compare-price>
                                          {% if settings.currency_code_enabled %}
                                            {{ variant.compare_at_price | money_with_currency }}
                                          {% else %}
                                            {{ variant.compare_at_price | money }}
                                          {% endif %}
                                        </span>
                                      {% endif %}
                                      {% unless sold_out %}
                                        <span data-regular-price data-price="{{ variant.price }}">
                                          {% if settings.currency_code_enabled %}
                                            {{ variant.price | money_with_currency }}
                                          {% else %}
                                            {{ variant.price | money }}
                                          {% endif %}
                                        </span>
                                      {% else %}
                                        <span>{{ 'products.product.sold_out' | t }}</span>
                                      {% endunless %}
                                    </p>
                                    {% if saved_price != blank %}
                                      <p class="product-bundles__save-price">
                                        {{ 'products.product.save_html' | t: amount: saved_price }}
                                      </p>
                                    {% endif %}
                                  </div>
                                </div>
                              </div>
                              <script type="application/json" data-product-variants>
                                {{ product.variants | json }}
                              </script>
                            {% endif %}
                          {% endfor %}
                          {% else %}
                            {% for i in (1..3) %}
                              <div
                                class="product-bundles__item m:display-flex m:items-center{% if block.settings.show_reviews %} product-bundles__item--has-reviews{% endif %}"
                                data-index="{{ forloop.index0 }}"
                              >
                                <span class="product-bundles__item-index m:inline-flex m:justify-center m:items-center">
                                  {{- forloop.index -}}
                                </span>
                                <a href="{{ product.url }}" class="m:block product-bundles__item-image">
                                  {{ 'product-' | append: forloop.index | placeholder_svg_tag: "m-placeholder-svg" }}
                                </a>
                                <div class="product-bundles__item-info m:display-flex m:flex-wrap md:m:flex-nowrap">
                                  <div class="product-bundles__item-details m:w-full">
                                    <h3 class="h4">
                                      <a href="#"> {{ 'homepage.onboarding.product_title' | t }}</a>
                                    </h3>
                                  </div>
                                  <div class="product-bundles__item-prices md:m:text-right">
                                    <p>
                                      <span data-regular-price>{{ 1999 | money }}</span>
                                    </p>
                                  </div>
                                </div>
                              </div>
                            {% endfor %}
                          {% endif %}
                        </div>
                        <div class="product-bundles__cta">
                          <button
                            class="m-button m-button--primary m-spinner-button m:w-full m:font-bold m:uppercase m:text-sm m:display-flex m:justify-center m:items-center"
                            data-add-bundle
                          >
                            <span>{{ 'products.product.add_to_cart' | t }}</span>
                            <span class="m-button-word-separator">|</span>
                            <span data-total-price="{{ total_price }}">{{ total_price | money }}</span>
                            <span class="m-spinner-icon">
                              <svg
                                class="animate-spin m-svg-icon--medium"
                                xmlns="http://www.w3.org/2000/svg"
                                viewBox="0 0 24 24"
                                fill="none"
                              >
                                <circle  cx="12" cy="12" r="10" stroke="currentColor" stroke-width="4"></circle>
                                <path fill="currentColor" d="M4 12a8 8 0 018-8V0C5.373 0 0 5.373 0 12h4zm2 5.291A7.962 7.962 0 014 12H0c0 3.042 1.135 5.824 3 7.938l3-2.647z"></path>
                              </svg>
                            </span>
                          </button>
                        </div>
                        <div class="product-bundles__error"></div>
                      </div>
                    {% when 'lookbook' %}
                      {{ 'section-lookbook.css' | asset_url | stylesheet_tag }}
                      {% render 'lookbook-card',
                        block: block,
                        image_aspect_ratio: 'original',
                        index: forloop.index,
                        animated: animated,
                        animation_effect: animation_effect,
                        card_color_scheme: block.settings.lookbook_item_color
                      %}
                    {% when 'countdown' %}
                      <div
                        class="m-countdown m-countdown--custom-content{% if animated %} m-scroll-trigger animate--{{ animation_effect }}{% endif %} m:justify-center md:m:justify-{{ block.settings.align }}"
                        {% if animated %}
                          data-cascade
                          style="--animation-order: {{ forloop.index }};"
                        {% endif %}
                      >
                        <div class="m-countdown__inner m:text-center">
                          {% if block.settings.heading != blank %}
                            <h3 class="m-countdown__heading {{ block.settings.text_size }}">
                              {{ block.settings.heading }}
                            </h3>
                          {% endif %}
                          {% render 'countdown-timer',
                            time: block.settings.time,
                            extra_classes: 'm:text-color-body'
                          %}

                          {% capture arrow  %}<svg width="12" height="12" viewBox="0 0 12 12" fill="none" xmlns="http://www.w3.org/2000/svg"><path d="M4.5 2.25L8.25 6L4.5 9.75" stroke="CurrentColor" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"></path></svg>{% endcapture %}
                          {% capture triangle %}<svg width="12" height="12" viewBox="0 0 12 12" fill="none" xmlns="http://www.w3.org/2000/svg"><path d="M8.51531 5.73474L4.76531 1.98474C4.71287 1.93223 4.64602 1.89647 4.57324 1.88198C4.50046 1.86748 4.42501 1.87491 4.35645 1.90332C4.28789 1.93172 4.22931 1.97984 4.1881 2.04156C4.1469 2.10328 4.12494 2.17584 4.125 2.25005V9.75005C4.12494 9.82426 4.1469 9.89682 4.1881 9.95855C4.22931 10.0203 4.28789 10.0684 4.35645 10.0968C4.42501 10.1252 4.50046 10.1326 4.57324 10.1181C4.64602 10.1036 4.71287 10.0679 4.76531 10.0154L8.51531 6.26536C8.55018 6.23054 8.57784 6.18918 8.59671 6.14365C8.61558 6.09813 8.62529 6.04933 8.62529 6.00005C8.62529 5.95077 8.61558 5.90197 8.59671 5.85645C8.57784 5.81092 8.55018 5.76957 8.51531 5.73474Z" fill="currentColor"/></svg>{% endcapture %}
                          {% liquid 
                            assign is_button_icon = false
                            assign button_label = block.settings.button_label
                            if button_label contains '[arrow]' or button_label contains '[triangle]'
                              assign is_button_icon = true
                            endif
                          %}
                          {% if button_label != blank %}
                            <div class="m-countdown__button">
                              <a
                                href="{{ block.settings.button_link }}"
                                class="m-button {{ block.settings.button_style }} m:block"
                              >
                                {{- button_label | replace: '[arrow]', arrow | replace: '[triangle]', triangle -}}
                              </a>
                            </div>
                          {% endif %}
                        </div>
                      </div>
                    {% when 'html' %}
                      {% if block.settings.code != blank %}
                        {{ block.settings.code }}
                      {% endif %}
                    {% when 'liquid' %}
                      {% if block.settings.custom_liquid != blank %}
                        {{ block.settings.custom_liquid }}
                      {% endif %}
                    {% when 'newsletter' %}
                      <div
                        class="newsletter-form m:text-center{% if animated %} m-scroll-trigger animate--{{ animation_effect }}{% endif %}"
                        {% if animated %}
                          data-cascade
                          style="--animation-order: {{ forloop.index }};"
                        {% endif %}
                      >
                        {% if block.settings.heading != blank %}
                          <h3 class="newsletter-form__title {{ block.settings.text_size }}">
                            {{ block.settings.heading }}
                          </h3>
                        {% endif %}
                        {% if block.settings.description != blank %}
                          <div class="newsletter-form__description rte m:text-color-{{ block.settings.text_color }}">
                            {{ block.settings.description }}
                          </div>
                        {% endif %}
                        <div class="newsletter-form__form">
                          {% render 'newsletter-form',
                            custom_id: block.id,
                            button_label: block.settings.submit_button,
                            placeholder: block.settings.placeholder
                          %}
                        </div>
                      </div>
                    {% when 'social' %}
                      <div
                        class="social-profiles m:text-center{% if animated %} m-scroll-trigger animate--{{ animation_effect }}{% endif %}"
                        {% if animated %}
                          data-cascade
                          style="--animation-order: {{ forloop.index }};"
                        {% endif %}
                      >
                        {% if block.settings.heading != blank %}
                          <h3 class="social-profiles__title {{ heading_size }}">{{ block.settings.heading }}</h3>
                        {% endif %}
                        {% if block.settings.description != blank %}
                          <div class="social-profiles__description rte {{ desc_color }}">
                            {{ block.settings.description }}
                          </div>
                        {% endif %}
                        <div class="social-profiles__list m:text-center m:display-flex m:justify-center">
                          {% render 'social-media-links', extra_classes: 'm:text-color-body' %}
                        </div>
                      </div>
                    {%- when 'article' -%}
                      {{ 'component-article-card.css' | asset_url | stylesheet_tag }}
                      {%- assign article = articles[block.settings.article] -%}
                      {%- if article != empty -%}
                        {% render 'article-card',
                          article: article,
                          view: 'grid',
                          items_per_row: 1,
                          index: forloop.index,
                          animated: animated,
                          animation_effect: animation_effect
                        %}
                      {%- else -%}
                        {% render 'article-card-placeholder',
                          index: forloop.index,
                          animated: animated,
                          animation_effect: animation_effect
                        %}
                      {%- endif -%}
                    {% when 'handpicked_products' %}
                      {{ 'component-product-card.css' | asset_url | stylesheet_tag }}
                      {% liquid
                        assign title = block.settings.title
                        assign sub_title = block.settings.sub_title
                        assign text = block.settings.text
                        assign button_label = block.settings.button_label
                        assign content_empty = false
                        if title == blank and sub_title == blank and text == blank and button_label == blank
                          assign content_empty = true
                        endif

                        assign product_list = block.settings.product_list
                        assign product_to_show = block.settings.product_to_show | default: 8
                        assign items_per_row = block.settings.items_per_row
                        assign enable_slider = block.settings.enable_slider

                        if product_list != blank
                          if product_to_show <= items_per_row or product_list.products.size <= items_per_row
                            assign enable_slider = false
                          endif
                        else
                          if product_to_show <= items_per_row
                            assign enable_slider = false
                          endif
                        endif

                        assign mobile_layout = block.settings.mobile_layout
                        if enable_slider and mobile_layout == 'inherit'
                          assign mobile_layout = 'slider'
                        endif

                        assign show_pagination = block.settings.show_pagination
                        assign show_navigation = block.settings.show_navigation
                        assign navigation_position = block.settings.navigation_position
                        assign pagination_type = 'fraction'

                        assign tablet_items = items_per_row | at_most: 3
                        assign laptop_items = items_per_row | at_most: 4
                        
                        assign card_style = block.settings.pcard_layout
                        assign image_ratio = block.settings.pcard_image_ratio
                        assign show_vendor = block.settings.show_vendor
                        assign enable_swipe_mobile = block.settings.enable_swipe_mobile

                        if card_style == nil or card_style == blank
                          assign card_style = settings.pcard_layout
                        endif
                      %}
                      <m-handpicked-products
                        class="handpicked-products m-slider--pagination-fraction"
                        data-product-card-style="{{ card_style }}"
                        id="custom-content-handpicked-products-{{ block.id }}"
                        data-id="{{ block.id }}"
                        data-enable-slider="{{ enable_slider }}"
                        {% if enable_slider %}
                          data-items="{{ items_per_row }}"
                          data-laptop-items="{{ laptop_items }}"
                          data-tablet-items="{{ tablet_items }}"
                          data-mobile-items="2"
                          data-show-pagination="{{ show_pagination }}"
                          data-show-navigation="{{ show_navigation }}"
                          data-pagination-type="{{ pagination_type }}"
                          data-mobile-layout="{{ mobile_layout }}"
                        {% endif %}
                        style="
                          --column-gap: {{ block.settings.column_gap }}px; 
                          --column-gap-mobile: {{ block.settings.column_gap_mobile }}px; 
                          --row-gap: {{ block.settings.row_gap }}px; 
                          --row-gap-mobile: {{ block.settings.row_gap_mobile }}px; 
                          --items: {{ items_per_row }};
                        "
                      >
                        {% unless content_empty %}
                          <div
                            class="handpicked-products__header m-richtext m:text-{{ block.settings.content_alignment }}{% if animated %} m-scroll-trigger animate--{{ animation_effect }}{% endif %}"
                            {% if animated %}
                              data-cascade
                              style="--animation-order: {{ forloop.index }};"
                            {% endif %}
                          >
                            {% if block.settings.subtitle != blank %}
                              <p class="m-richtext__subtitle">{{ block.settings.subtitle }}</p>
                            {% endif %}
                            {% if block.settings.title != blank %}
                              <h3 class="m-richtext__title rte {{ heading_size }}">{{ block.settings.title }}</h3>
                            {% endif %}
                            {% if block.settings.text != blank %}
                              <div class="m-richtext__description rte {{ desc_color }}">
                                {{ block.settings.text }}
                              </div>
                            {% endif %}

                            {% capture arrow  %}<svg width="12" height="12" viewBox="0 0 12 12" fill="none" xmlns="http://www.w3.org/2000/svg"><path d="M4.5 2.25L8.25 6L4.5 9.75" stroke="CurrentColor" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"></path></svg>{% endcapture %}
                            {% capture triangle %}<svg width="12" height="12" viewBox="0 0 12 12" fill="none" xmlns="http://www.w3.org/2000/svg"><path d="M8.51531 5.73474L4.76531 1.98474C4.71287 1.93223 4.64602 1.89647 4.57324 1.88198C4.50046 1.86748 4.42501 1.87491 4.35645 1.90332C4.28789 1.93172 4.22931 1.97984 4.1881 2.04156C4.1469 2.10328 4.12494 2.17584 4.125 2.25005V9.75005C4.12494 9.82426 4.1469 9.89682 4.1881 9.95855C4.22931 10.0203 4.28789 10.0684 4.35645 10.0968C4.42501 10.1252 4.50046 10.1326 4.57324 10.1181C4.64602 10.1036 4.71287 10.0679 4.76531 10.0154L8.51531 6.26536C8.55018 6.23054 8.57784 6.18918 8.59671 6.14365C8.61558 6.09813 8.62529 6.04933 8.62529 6.00005C8.62529 5.95077 8.61558 5.90197 8.59671 5.85645C8.57784 5.81092 8.55018 5.76957 8.51531 5.73474Z" fill="currentColor"/></svg>{% endcapture %}
                            {% liquid 
                              assign is_button_icon = false
                              assign button_label = block.settings.button_label
                              if button_label contains '[arrow]' or button_label contains '[triangle]'
                                assign is_button_icon = true
                              endif
                            %}
                            {% if button_label != blank %}
                                <a
                                  href="{{ block.settings.button_link }}"
                                  class="m-richtext__button m-button {{ block.settings.button_style }} m:block"
                                >
                                  {{- button_label | replace: '[arrow]', arrow | replace: '[triangle]', triangle -}}
                                </a>
                            {% endif %}
                          </div>
                        {% endunless %}

                        <div 
                          class="handpicked-products__wrapper m-mixed-layout{% if mobile_layout != 'slider' %} m-mixed-layout--mobile-grid{% if enable_swipe_mobile %} m-mixed-layout--mobile-scroll{% endif %}{% endif %}"
                          {% if section.settings.mobile_layout != 'inherit' %}
                            style="
                            --grid-columns-mobile: {{ block.settings.mobile_layout | replace: 'col-1', '1' | replace: 'col-2', '2' }};
                            --mobile-column-width: {{ block.settings.mobile_layout | replace: 'col-1', '66.66vw' | replace: 'col-2', '40vw' }};
                            "
                          {% endif %}
                        >
                          <div class="m-mixed-layout__wrapper{% if enable_slider %} swiper swiper--equal-height{% endif %}">
                            <div
                              class="m-mixed-layout__inner m:grid md:m:grid-{{ tablet_items }}-cols lg:m:grid-{{ laptop_items }}-cols xl:m:grid-{{ items_per_row }}-cols{% if enable_slider %} swiper-wrapper{% endif %}"
                            >
                              {% if product_list != blank %}
                                {% for product in product_list limit: product_to_show %}
                                  <div class="m:column{% if enable_slider %} swiper-slide{% endif %}">
                                    {% liquid
                                      case card_style
                                        when '1'
                                          render 'product-card-1', product: product, image_ratio: image_ratio, show_vendor: show_vendor, index: forloop.index, animated: animated, animation_effect: animation_effect
                                        when '2'
                                          render 'product-card-2', product: product, image_ratio: image_ratio, show_vendor: show_vendor, index: forloop.index, animated: animated, animation_effect: animation_effect
                                        when '3'
                                          render 'product-card-3', product: product, image_ratio: image_ratio, show_vendor: show_vendor, index: forloop.index, animated: animated, animation_effect: animation_effect
                                      endcase
                                    %}
                                  </div>
                                {% endfor %}
                              {% else %}
                                {% for i in (1..product_to_show) %}
                                  <div class="m:column{% if enable_slider %} swiper-slide{% endif %}">
                                    {% render 'product-card-placeholder', index: forloop.index, animated: animated, animation_effect: animation_effect %}
                                  </div>
                                {% endfor %}
                              {% endif %}
                            </div>
                          </div>
                          {% if enable_slider and show_pagination or show_navigation %}
                            {% render 'slider-controls',
                              show_pagination: show_pagination,
                              show_nav: show_navigation,
                              pagination_type: pagination_type,
                              navigation_position: navigation_position, 
                              nav_class: 'm:opacity-0'
                            %}
                          {% endif %}
                        </div>
                      </m-handpicked-products>

                    {% when 'image_comparison' %}
                      {{ 'section-image-comparison.css' | asset_url | stylesheet_tag }}
                      {% liquid
                        assign title = block.settings.title
                        assign sub_title = block.settings.sub_title
                        assign text = block.settings.text
                        assign content_empty = false
                        if title == blank and sub_title == blank and text == blank
                          assign content_empty = true
                        endif

                        assign layout = block.settings.layout
                        assign image_height = block.settings.image_height

                        if image_height == 'adapt'
                          assign aspect_ratio = block.settings.before_image.aspect_ratio | default: 2.333
                          assign aspect_ratio_mobile = block.settings.before_image_mobile.aspect_ratio | default: aspect_ratio
                        endif
                      %}
                      
                        {% unless content_empty %}
                          <div
                            class="image-comparison__header m-richtext m:text-{{ block.settings.content_alignment }}{% if animated %} m-scroll-trigger animate--{{ animation_effect }}{% endif %}"
                            {% if animated %}
                              data-cascade
                              style="--animation-order: {{ forloop.index }};"
                            {% endif %}
                          >
                            {% if block.settings.subtitle != blank %}
                              <p class="m-richtext__subtitle">{{ block.settings.subtitle }}</p>
                            {% endif %}
                            {% if block.settings.title != blank %}
                              <h3 class="m-richtext__title rte {{ heading_size }}">{{ block.settings.title }}</h3>
                            {% endif %}
                            {% if block.settings.text != blank %}
                              <div class="m-richtext__description rte {{ desc_color }}">
                                {{ block.settings.text }}
                              </div>
                            {% endif %}
                          </div>
                        {% endunless %}

                        <m-image-comparison
                          class="m-image-comparison{% if animated %} m-scroll-trigger animate--{{ animation_effect }}{% endif %}"
                          data-layout="{{ layout }}"
                          {% if animated %}
                            data-animation
                          {% endif %}
                        >
                          {% liquid
                            assign image = block.settings.before_image
                            assign image_mobile = block.settings.before_image_mobile
                            assign heading = block.settings.before_heading
                            assign image_class = ''
                            if image_mobile != blank
                              assign image_class = image_class | append: ' has-mobile-image'
                            endif
                          %}
                          {% if animated %}<span class="m-image-comparison__slider-animation"></span>{% endif %}
                          <div class="m-image-comparison__before m-image-comparison--media">
                            {% if heading != blank %}
                              <p class="m-image-comparison__heading h6">{{ heading }}</p>
                            {% endif %}
                            <div
                              class="m-image-comparison__wrapper m-image-comparison--{{ image_height }}{{ image_class }}"
                              {% if image_height == 'adapt' %}
                                style="--aspect-ratio: {{ aspect_ratio }}; --aspect-ratio-mobile: {{ aspect_ratio_mobile }}"
                              {% endif %}
                            >
                              {% if image != blank %}
                                {% assign image_height = image.width | divided_by: image.aspect_ratio %}
                                {{
                                  image
                                  | image_url: width: 1500
                                  | image_tag:
                                    loading: 'lazy',
                                    class: 'm-image-comparison__image-desktop',
                                    width: image.width,
                                    height: image_height,
                                    sizes: '100vw',
                                    widths: '375, 550, 750, 1100, 1500, 1780, 2000, 3000, 3840',
                                    alt: image.alt
                                  | escape
                                }}
                              {% else %}
                                {{
                                  'lifestyle-1'
                                  | placeholder_svg_tag: 'm-placeholder-svg m-image-comparison__image-desktop'
                                }}
                              {% endif %}
                              {% if image_mobile != blank %}
                                {% assign image_mobile_height = image_mobile.width
                                  | divided_by: image_mobile.aspect_ratio
                                %}
                                {{
                                  image_mobile
                                  | image_url: width: 750
                                  | image_tag:
                                    loading: 'lazy',
                                    class: 'm-image-comparison__image-mobile',
                                    width: image_mobile.width,
                                    height: image_mobile_height,
                                    sizes: '100vw',
                                    widths: '375, 550, 750',
                                    alt: image_mobile.alt
                                  | escape
                                }}
                              {% endif %}
                            </div>
                          </div>

                          {% liquid
                            assign image = block.settings.after_image
                            assign image_mobile = block.settings.after_image_mobile
                            assign heading = block.settings.after_heading
                            assign image_class = ''
                            if image_mobile != blank
                              assign image_class = image_class | append: ' has-mobile-image'
                            endif
                          %}
                          <div class="m-image-comparison__after m-image-comparison--media">
                            {% if heading != blank %}
                              <p class="m-image-comparison__heading h6">{{ heading }}</p>
                            {% endif %}
                            <div
                              class="m-image-comparison__wrapper m-image-comparison--{{ image_height }}{{ image_class }}"
                              {% if image_height == 'adapt' %}
                                style="--aspect-ratio: {{ aspect_ratio }}; --aspect-ratio-mobile: {{ aspect_ratio_mobile }}"
                              {% endif %}
                            >
                              {% if image != blank %}
                                {% assign image_height = image.width | divided_by: image.aspect_ratio %}
                                {{
                                  image
                                  | image_url: width: 1500
                                  | image_tag:
                                    loading: 'lazy',
                                    class: 'm-image-comparison__image-desktop',
                                    width: image.width,
                                    height: image_height,
                                    sizes: '100vw',
                                    widths: '375, 550, 750, 1100, 1500, 1780, 2000, 3000, 3840',
                                    alt: image.alt
                                  | escape
                                }}
                              {% else %}
                                {{
                                  'lifestyle-2'
                                  | placeholder_svg_tag: 'm-placeholder-svg m-image-comparison__image-desktop'
                                }}
                              {% endif %}
                              {% if image_mobile != blank %}
                                {% assign image_mobile_height = image_mobile.width
                                  | divided_by: image_mobile.aspect_ratio
                                %}
                                {{
                                  image_mobile
                                  | image_url: width: 750
                                  | image_tag:
                                    loading: 'lazy',
                                    class: 'm-image-comparison__image-mobile',
                                    width: image_mobile.width,
                                    height: image_mobile_height,
                                    sizes: '100vw',
                                    widths: '375, 550, 750',
                                    alt: image_mobile.alt
                                  | escape
                                }}
                              {% endif %}
                            </div>
                          </div>

                          <button class="m-image-comparison__button" aria-label="Drag">
                            <span>
                              <svg
                                xmlns="http://www.w3.org/2000/svg"
                                aria-hidden="true"
                                focusable="false"
                                fill="none"
                                viewBox="0 0 11 16"
                              >
                                <path fill-rule="evenodd" clip-rule="evenodd" d="M0 0.5C0 0.223858 0.223858 0 0.5 0C0.776142 0 1 0.223858 1 0.5V15.5C1 15.7761 0.776142 16 0.5 16C0.223858 16 0 15.7761 0 15.5V0.5ZM5 0.5C5 0.223858 5.22386 0 5.5 0C5.77614 0 6 0.223858 6 0.5V15.5C6 15.7761 5.77614 16 5.5 16C5.22386 16 5 15.7761 5 15.5V0.5ZM11 0.5C11 0.223858 10.7761 0 10.5 0C10.2239 0 10 0.223858 10 0.5V15.5C10 15.7761 10.2239 16 10.5 16C10.7761 16 11 15.7761 11 15.5V0.5Z" fill="currentColor"></path>
                              </svg>
                            </span>
                          </button>
                        </m-image-comparison>
                      
                  {% endcase %}
                </div>
              </div>
            {% endfor %}
          </m-custom-content>
        </div>
      {% endif %}
    </div>
  </div>
</div>

{% schema %}
{
  "name": "Custom content",
  "disabled_on": {
    "groups": [
      "aside"
    ]
  },
  "settings": [
    {
      "type": "header",
      "content": "General"
    },
    {
      "type": "color_scheme",
      "id": "color_scheme",
      "default": "scheme_1",
      "label": "Color scheme"
    },
    {
      "type": "header",
      "content": "Layout"
    },
    {
      "type": "checkbox",
      "id": "boxed_layout",
      "label": "Enable boxed layout",
      "default": false
    },
    {
      "type": "color_scheme",
      "id": "boxed_color_scheme",
      "default": "scheme_1",
      "label": "Color scheme",
      "info": "Only works if Boxed layout is enabled."
    },
    {
      "type": "header",
      "content": "Section header"
    },
    {
      "type": "checkbox",
      "id": "header_divider",
      "label": "Show header divider",
      "default": false
    },
    {
      "type": "select",
      "id": "header_alignment",
      "label": "Text alignment",
      "default": "center",
      "options": [
        {
          "value": "left",
          "label": "Left"
        },
        {
          "value": "center",
          "label": "Center"
        }
      ]
    },
    {
      "type": "select",
      "id": "header_mobile_alignment",
      "label": "Mobile text alignment",
      "options": [
        {
          "value": "left",
          "label": "Left"
        },
        {
          "value": "center",
          "label": "Center"
        }
      ]
    },
    {
      "type": "textarea",
      "id": "heading",
      "label": "Heading",
      "default": "Custom content"
    },
    {
      "type": "select",
      "id": "heading_size",
      "label": "Heading size",
      "options": [
        {
          "value": "h2",
          "label": "H2"
        },
        {
          "value": "h3",
          "label": "H3"
        },
        {
          "value": "h4",
          "label": "H4"
        },
        {
          "value": "h5",
          "label": "H5"
        }
      ],
      "default": "h2"
    },
    {
      "type": "inline_richtext",
      "id": "subheading",
      "label": "Subheading"
    },
    {
      "type": "richtext",
      "id": "description",
      "label": "Description"
    },
    {
      "type": "select",
      "id": "text_color",
      "label": "Description style",
      "options": [
        {
          "value": "body",
          "label": "Body text"
        },
        {
          "value": "subtext",
          "label": "Subtext"
        }
      ],
      "default": "body"
    },
    {
      "type": "header",
      "content": "Column gap settings"
    },
    {
      "type": "range",
      "id": "gap",
      "label": "Column gap",
      "default": 30,
      "min": 0,
      "max": 100,
      "step": 1,
      "unit": "px"
    },
    {
      "type": "range",
      "id": "row_gap",
      "label": "Row gap",
      "default": 40,
      "min": 0,
      "max": 100,
      "step": 1,
      "unit": "px"
    },
    {
      "type": "range",
      "id": "gap_mobile",
      "label": "Column gap mobile",
      "default": 16,
      "min": 0,
      "max": 50,
      "step": 1,
      "unit": "px"
    },
    {
      "type": "range",
      "id": "row_gap_mobile",
      "label": "Row gap mobile",
      "default": 24,
      "min": 0,
      "max": 50,
      "step": 1,
      "unit": "px"
    },
    {
      "type": "header",
      "content": "Section padding"
