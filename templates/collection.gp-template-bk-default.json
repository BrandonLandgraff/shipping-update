        "liquid_THE6hP": {
          "settings": {
            "alignment": "center",
            "animations": "inherit",
            "block_custom_class": "",
            "custom_liquid": "{%- if collection.metafields.custom.promotion_banner.value != blank -%}\n  <div style=\"display: flex; justify-content: center; align-items: center;\">\n    <img srcset=\"{{ collection.metafields.custom.promotion_banner.value | img_url:'master' }}\" style=\"height: 150px; width: auto; border-radius: var(--boxed-radius, 0px);\" alt=\"{{ collection.title }} - ECIGONE\" loading=\"lazy\">\n    <noscript>\n      <img loading=\"lazy\" src=\"{{ collection.metafields.custom.collection_picture_full_width | img_url:'master' }}\" style=\"height: 150px; width: auto; border-radius: var(--boxed-radius, 0px);\" alt=\"{{ collection.title }} - ECIGONE\">\n    </noscript>\n  </div>\n{%- endif -%}",
