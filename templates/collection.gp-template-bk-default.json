        "liquid_eBpPVg": {
          "settings": {
            "alignment": "center",
            "animations": "inherit",
            "block_custom_class": "",
            "custom_liquid": "<div class=\"slick-slider\">\n  {% for collection in collections %}\n    {% if collection.tags contains 'custom.collectionoffer' %}\n      <div class=\"slick-slide\">\n        <div class=\"collection-container\">\n          <div class=\"collection-image\">\n            <img src=\"{{ collection.image.src | img_url: '300x300' }}\" alt=\"{{ collection.title }}\" class=\"rounded-image\">\n          </div>\n          <div class=\"collection-text\">\n            <p>{{ collection.title }}</p>\n          </div>\n        </div>\n      </div>\n    {% endif %}\n  {% endfor %}\n</div>\n\n<style>\n  .slick-slider {\n    width: 100%;\n    overflow: hidden;\n  }\n  .slick-slide {\n    float: left;\n    padding: 0 10px;\n    box-sizing: border-box;\n  }\n  .collection-container {\n    border-radius: 5px;\n    overflow: hidden;\n    box-shadow: 0 0 5px rgba(0,0,0,0.2);\n    background: #fff;\n    text-align: center;\n    width: 250px; /* Adjust as per your design */\n    margin: 0 auto; /* Centers the container */\n  }\n  .collection-image {\n    padding: 10px;\n  }\n  .rounded-image {\n    border-radius: 50%;\n    border: 2px solid #ccc; /* Adjust border style */\n    width: 100px; /* Adjust image size */\n    height: 100px; /* Adjust image size */\n    display: block;\n    margin: 0 auto;\n  }\n  .collection-text {\n    padding: 10px;\n  }\n</style>\n\n<script>\n  $(document).ready(function(){\n    $('.slick-slider').slick({\n      slidesToShow: 3, // Adjust number of slides shown at once\n      slidesToScroll: 1,\n      autoplay: true,\n      autoplaySpeed: 2000, // Adjust autoplay speed (milliseconds)\n      dots: true, // Enable dots navigation\n      responsive: [\n        {\n          breakpoint: 768,\n          settings: {\n            slidesToShow: 2\n          }\n        },\n        {\n          breakpoint: 480,\n          settings: {\n            slidesToShow: 1\n          }\n        }\n      ]\n    });\n  });\n</script>",
            "width": "100%"
          },
          "type": "liquid"
        },
