      ],
      "settings": {
        "custom_class": "m-section m-custom-content  m-section--padding m-color-background-1 m-gradient m-section--boxed m-section--header-separated",
        "custom_liquid": "{%- if collection.metafields.custom.description_title.value != blank -%}\n<div class=\"m-page-width\">\n  <div class=\"m-section__wrapper m-gradient m-color-scheme-9b802ae1-d821-4057-8103-ed71b8f01c72\">\n    <div class=\"m-section__header m:text-center sm:m:text-center\">\n      <div class=\"m-section__description m:text-color-body  m-scroll-trigger animate--fade-in-up\"><h2>{{ collection.metafields.custom.description_title.value }}</h2></div>\n    </div>\n    <div class=\"m-section__content\">\n      <m-custom-content class=\"m-custom-content__wrapper m:flex m:flex-wrap\">\n        <div class=\"m-custom-content__block m-custom-content__block-text m:column lg:m:w-full m:w-full m:display-flex m:items-start \">\n          <div class=\"m-custom-content__block-inner m:w-full\">\n            <div class=\"m-richtext m:text-left m-scroll-trigger animate--fade-in-up\" style=\"--animation-order: 1;\" data-cascade=\"\">\n              <div class=\"m-richtext__description rte m:text-color-body\"><div class=\"metafield-rich_text_field\"><p>{{ collection.metafields.custom.description_1 | metafield_tag }}</p></div></div>\n            </div>\n          </div>\n        </div>\n        <div class=\"m-custom-content__block m-custom-content__block-text m:column lg:m:w-1/2 m:w-full m:display-flex m:items-start \">\n          <div class=\"m-custom-content__block-inner m:w-full\">\n            <div class=\"m-richtext m:text-left m-scroll-trigger animate--fade-in-up\" style=\"--animation-order: 2;\" data-cascade=\"\">\n              <p class=\"m-richtext__subtitle\"><h2>{{ collection.metafields.custom.subheading_2.value }}</h2></p>\n              <div class=\"m-richtext__description rte m:text-color-body\"><div class=\"metafield-rich_text_field\"><p>{{ collection.metafields.custom.sub_header_2_text | metafield_tag }}</p></div></div>\n            </div>\n          </div>\n        </div>\n        <div class=\"m-custom-content__block m-custom-content__block-text m: